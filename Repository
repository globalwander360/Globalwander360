this.getThrowbacks().pipe(
  concatMap(() =>
    this.initiationEventDataservice.getTransactionPropertiesByEventId(data.id, keys)
  )
).subscribe((transactionProperties: TransactionProperties[]) => {
  const completionMotiveData = transactionProperties.filter(
    (p: any) => p.categoryName === 'COMPLETION_MOTIVE'
  );

  const completionMotives = completionMotiveData.map(motive => ({
    id: motive.completionMotiveId,
    comment: motive.completionMotiveComment
  }));

  return onload({
    ...data,
    completionMotives
  });
});
