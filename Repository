case BUFFER_FUNCTION.GET_TRANSACTION_PROPERTIES:
  this.getThrowbacks().pipe(
    concatMap(() => 
      this.initiationEventDataservice.getTransactionPropertiesByEventId(data.id, keys)
    )
  ).subscribe((transactionProperties: TransactionProperties[]) => {

    const motiveLabels = transactionProperties
      .filter(p => p.propertyType === 'COMPLETION_MOTIVE') // ðŸ‘ˆ Filter for Completion Motive only
      .map(p => p.propertyName)
      .join(',');

    return onLoad({ ...data, transactionProperties: motiveLabels });

  });
  break;
