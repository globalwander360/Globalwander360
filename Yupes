@Test
void getPropertyNamesByEventIdAndType_ShouldReturnValidList() {
    // Arrange
    String eventId = "EVT123";
    String propertyType = "STATUS";

    List<Long> mockIds = Arrays.asList(1L, 2L, 3L);

    when(transactionPropertiesRepository.getPropertyNamesByEventIdAndType(eventId, propertyType))
        .thenReturn(mockIds);

    // Act
    List<Long> result = requestService.getPropertyNamesByEventIdAndType(eventId, propertyType);

    // Assert
    assertNotNull(result);
    assertEquals(3, result.size());
    assertEquals(Long.valueOf(1), result.get(0));
    assertEquals(Long.valueOf(2), result.get(1));
    assertEquals(Long.valueOf(3), result.get(2));
}
